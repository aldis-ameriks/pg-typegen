{
  "name": "pg-typegen",
  "version": "3.9.0",
  "description": "Generate TypeScript type definitions from Postgres database",
  "type": "module",
  "module": "src/index.js",
  "main": "cjs/src/index.js",
  "exports": {
    "types": "./types/index.d.ts",
    "import": "./src/index.js",
    "default": "./cjs/src/index.js"
  },
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "bin": {
    "pg-typegen": "cjs/src/index.js"
  },
  "scripts": {
    "test": "node --test --experimental-test-coverage --experimental-test-snapshots --test-coverage-lines=100 --test-coverage-branches=100 --test-coverage-include='src/**/*.js' --import=./test/helpers/setup.js test/*.js",
    "test:cjs": "node --test --experimental-test-coverage --experimental-test-snapshots --test-coverage-lines=100 --test-coverage-branches=100 --test-coverage-include='cjs/src/**/*.js' --import=./cjs/test/helpers/setup.js cjs/test/*.js",
    "test:types": "tsd",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "transpile:cjs": "node ./transpile.cjs.js && npm run lint:fix"
  },
  "author": "Aldis Ameriks <aldis.ameriks@gmail.com>",
  "keywords": [
    "typescript",
    "types",
    "database",
    "postgres",
    "schema",
    "codegen",
    "typegen"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aldis-ameriks/pg-typegen.git"
  },
  "homepage": "https://github.com/aldis-ameriks/pg-typegen#readme",
  "bugs": {
    "url": "https://github.com/aldis-ameriks/pg-typegen/issues"
  },
  "license": "MIT",
  "dependencies": {
    "postgres": "^3.4.4"
  },
  "devDependencies": {
    "standard": "^17.1.0",
    "tsd": "^0.31.2"
  },
  "tsd": {
    "directory": "types"
  }
}
